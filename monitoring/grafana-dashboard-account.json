{
  "dashboard": {
    "title": "CBSA Migration - Account Domain",
    "tags": ["cbsa", "account", "banking"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Account Operations - Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{uri=~\".*account.*\"}[5m])",
            "legendFormat": "{{method}} {{uri}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 2,
        "title": "Account Operations - Response Time (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{uri=~\".*account.*\"}[5m]))",
            "legendFormat": "{{method}} {{uri}} p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_server_requests_seconds_bucket{uri=~\".*account.*\"}[5m]))",
            "legendFormat": "{{method}} {{uri}} p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 3,
        "title": "Account Repository - Query Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(jdbc_connections_active[5m])",
            "legendFormat": "DB Operations/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Operations"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "title": "Account Service - Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{uri=~\".*account.*\",status=~\"5..\"}[5m])",
            "legendFormat": "{{uri}} 5xx errors",
            "refId": "A"
          },
          {
            "expr": "rate(http_server_requests_seconds_count{uri=~\".*account.*\",status=~\"4..\"}[5m])",
            "legendFormat": "{{uri}} 4xx errors",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "alert": {
          "name": "High Account Error Rate",
          "conditions": [
            {
              "evaluator": {"params": [0.05], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "message": "Account domain error rate exceeds 5%",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 5,
        "title": "Account Count",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "cbsa_account_count",
            "refId": "A"
          }
        ],
        "format": "short",
        "valueName": "current"
      },
      {
        "id": 6,
        "title": "System CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "system_cpu_usage",
            "legendFormat": "System CPU",
            "refId": "A"
          },
          {
            "expr": "process_cpu_usage",
            "legendFormat": "Process CPU",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "CPU Usage"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      }
    ]
  }
}
